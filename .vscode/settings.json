{
	"eslint.validate": [
		"javascript",
	],
	"files.exclude": {
		"LICENSE": true,
		"*.lnk": true,
		"*.log": true,
		".npmrc": true,
		".editorconfig": true,
		"package-lock.json": true,
		"lib": true,
		"node_modules": true,
		"*/**/node_modules": false,
		"packages/*/node_modules": true,
		"test/fixtures/*/package-lock.json": true,
		"test/fixtures/*/tsconfig.json": true,
		"test/fixtures/*/lib": true,
		"test/fixtures/*/dist": true
	}
}
