{
  "private": true,
  "name": "@wildpeaks/tsconfig",
  "version": "1.1.0",
  "description": "Base tsconfig packages",
  "author": "Cecile Muller",
  "license": "MIT",
  "homepage": "https://github.com/wildpeaks/packages-typescript-config#readme",
  "repository": "https://github.com/wildpeaks/packages-typescript-config",
  "bugs": {
    "url": "https://github.com/wildpeaks/packages-typescript-config/issues"
  },
  "eslintIgnore": [
    "packages"
  ],
  "eslintConfig": {
    "extends": "@wildpeaks/commonjs"
  },
  "greenkeeper": {
    "commitMessages": {
      "dependencyUpdate": "fix: Update ${dependency}",
      "devDependencyUpdate": "chore: Update ${dependency}"
    }
  },
  "scripts": {
    "setup": "node test/setup.js",
    "test:lint": "mocha test/lint.spec.js",
    "test:node": "mocha test/node.spec.js",
    "test:web": "mocha test/web.spec.js",
    "test:class": "mocha test/class.spec.js",
    "test": "npm run test:lint && npm run test:node && npm run test:web"
  },
  "peerDependencies": {
    "typescript": "*"
  },
  "devDependencies": {
    "@wildpeaks/async": "1.2.2",
    "@wildpeaks/eslint-config-commonjs": "7.3.0",
    "@wildpeaks/webpack-config-web": "4.11.0",
    "ajv": "6.10.2",
    "eslint": "6.7.2",
    "express": "4.17.1",
    "fs-extra": "8.1.0",
    "mocha": "6.2.2",
    "preact": "10.1.0",
    "puppeteer": "2.0.0",
    "recursive-readdir": "2.2.2",
    "typescript": "3.7.2",
    "webpack": "4.41.2",
    "webpack-cli": "3.3.10"
  }
}
